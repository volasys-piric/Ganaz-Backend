    "async-lock": "^1.1.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "csv-parser": "^1.12.0",
    "express": "^4.15.5",
    "express-jwt": "^5.3.0",
    "fs-extra-promise": "^1.0.1",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^5.0.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
